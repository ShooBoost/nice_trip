AAAA,M,WAAA,C,wBAEa,IAAI,CAAC,M,KAalB,mBAAA,CAWK,IAXL,EAAA,WAAA,EAWK,gBAAA,CADA,IAAI,CAAC,IACL,CAXL,EAUc;AAAA;AAVd,K,CA5BF,E;;AAAA,M,WAAA,C,CADF,C;GA0CM,CA1CN,E;;AAAA,GA2CI,EA5CN,CA6CQ,CA/CV,C,uCAiDA,mBAAA,CAAA,wBAAA,C,EACA,mBAAA,CAAA,4BAAA,C,EAEiC,KAAA,CAAA,W,kBAAjC,mBAAA,CAmBU,SAnBV,EAAA,WAAA,EAmBU,CAlBR,WAkBQ,EAjBR,mBAAA,CAgBM,KAhBN,EAAA,WAAA,EAgBM,E,kBAfJ,mBAAA,CAcM,SAdN,EAcM,IAdN,EAcM,WAAA,CAZgB,KAAA,CAAA,eAYhB,EAZ+B,UAA3B,IAA2B,EAArB,CAAqB,EAApB;yBAFjB,mBAAA,CAcM,KAdN,EAcM;AAbJ,MAAA,KAAK,EAAC,qCAaF;AAXH,MAAA,GAAG,EAAE;AAWF,KAdN,E,CAKE,YAAA,CAQE,6BARF,EAQE;AAPA,MAAA,KAAK,EAAC,OAON;AANC,MAAA,OAAO,EAAE,IAAA,CAAA,MAAA,CAAO,KAAP,CAAa,KAMvB;AALC,MAAA,MAAM,EAAE,IAAI,CAAC,EAKd;AAJC,MAAA,MAAM,EAAE,IAAI,CAAC,OAAL,CAAa,WAAb,IAA4B,KAAA,CAAA,SAIrC;AAHC,MAAA,SAAS,EAAE,KAAA,CAAA,SAGZ;AAFC,MAAA,QAAQ,EAAE,IAAI,CAAC,IAEhB;AADC,MAAA,QAAQ,EAAE,IAAI,CAAC;AAChB,KARF,E,IAAA,E;;AAAA,M,oEAAA,C,CALF,C;GAcM,CAdN,E;;AAAA,GAeI,EAhBN,CAiBQ,CAnBV,C,uCAqBA,mBAAA,CAAA,0BAAA,C","sourcesContent":["<template>\n  <!-- Breadcrumb - start -->\n  <section class=\"container pt-5dot5 pt-lg-8dot75 mb-1 mb-lg-2dot5\">\n    <Breadcrumb :apiType=\"$route.query.theme\" />\n  </section>\n\n  <!-- Breadcrumb - end -->\n\n  <!-- searching bar - start -->\n  <section class=\"container mb-1dot5 mb-3dot75\">\n    <div class=\"row\">\n      <!-- 縣市 - start -->\n      <div class=\"col-12 col-lg-3\">\n        <div class=\"dropdown mb-0dot5\">\n          <button\n            class=\"\n              btn btn-lg btn-outline-primary\n              border-gray\n              dropdown-toggle\n              w-100\n              fw-bold\n              d-flex\n              justify-content-between\n              align-items-center\n              px-2\n              py-0dot75\n              fs-6\n            \"\n            type=\"button\"\n            id=\"dropdownMenuButton1\"\n            data-bs-toggle=\"dropdown\"\n            aria-expanded=\"false\"\n          >\n            {{ city || \"選擇縣市\" }}\n          </button>\n          <ul\n            class=\"dropdown-menu w-100 mt-0dot5 p-0\"\n            aria-labelledby=\"dropdownMenuButton1\"\n          >\n            <li v-for=\"(item, i) in cityList\" :key=\"i\">\n              <hr v-if=\"i !== 0\" class=\"dropdown-divider m-0\" />\n              <a\n                @click=\"setCityParameterForGetTdxData(item)\"\n                class=\"dropdown-item py-0dot75 px-2\"\n                >{{ item.CityName }}</a\n              >\n            </li>\n          </ul>\n        </div>\n      </div>\n      <!-- 縣市 - end -->\n      <!-- 類別 - start -->\n      <div class=\"col-12 col-lg-3\">\n        <div class=\"dropdown mb-0dot5\">\n          <button\n            class=\"\n              btn btn-lg btn-outline-primary\n              border-gray\n              dropdown-toggle\n              w-100\n              fw-bold\n              d-flex\n              justify-content-between\n              align-items-center\n              px-2\n              py-0dot75\n              fs-6\n            \"\n            type=\"button\"\n            id=\"dropdownMenuButton1\"\n            data-bs-toggle=\"dropdown\"\n            aria-expanded=\"false\"\n          >\n            {{ category || \"選擇類別\" }}\n          </button>\n          <ul\n            class=\"dropdown-menu w-100 mt-0dot5 p-0\"\n            aria-labelledby=\"dropdownMenuButton1\"\n          >\n            <li v-for=\"(item, i) in categoriesList\" :key=\"i\">\n              <hr v-if=\"i !== 0\" class=\"dropdown-divider m-0\" />\n              <a\n                @click=\"setClassParameterForGetTdxData(item.name)\"\n                class=\"dropdown-item py-0dot75 px-2\"\n                >{{ item.name }}</a\n              >\n            </li>\n          </ul>\n        </div>\n      </div>\n      <!-- 類別 - end -->\n      <!-- keywords - start -->\n      <div class=\"col-12 col-lg-4\">\n        <input\n          class=\"\n            form-control form-control-lg\n            bg-gray-light\n            px-2\n            py-0dot75\n            mb-0dot5\n            focusPrimary\n            fs-6\n          \"\n          type=\"text\"\n          placeholder=\"你想去哪裡？請輸入關鍵字\"\n          aria-label=\"搜尋欄，請填入關鍵字進行搜尋\"\n          v-model=\"keywords\"\n        />\n      </div>\n      <!-- keywords - end -->\n      <div class=\"col-12 col-lg-2\">\n        <button\n          @click=\"getSearchingResults\"\n          class=\"btn btn-primary w-100 btn-lg text-white letterSpacingLg fs-6\"\n          type=\"submit\"\n        >\n          <span class=\"material-icons align-text-top\"> search </span>\n          搜尋\n        </button>\n      </div>\n    </div>\n  </section>\n  <!-- searching bar - end -->\n\n  <!-- categoriesList - start -->\n  <section class=\"container\" v-if=\"!showResults\">\n    <h2 class=\"fs-4 fsLg1dot5 fw-normal m-0 mb-0dot5 mb-lg-0dot75\">熱門分類</h2>\n    <div class=\"row\">\n      <div class=\"col-6 col-lg-3 mb-0dot75\" v-for=\"(item, i) in categoriesList\" :key=\"i\">\n        <div\n          @click=\"getResultsOfSpecificCategory(item)\"\n          class=\"\n            w-100\n            paddingTop53Percent\n            paddingTopLg36Percent\n            position-relative\n            text-white\n            radius24px\n            overflow-hidden\n          \"\n        >\n          <!-- 如果圖片網址無效，要用備用圖片 -->\n          <img\n            :src=\"backupImg\"\n            v-mainImg=\"item.imgUrl\"\n            :alt=\"item.name\"\n            class=\"\n              h-100\n              w-100\n              position-absolute\n              top-50\n              start-0\n              translate-middle-y\n              objectFitCover\n              brightnessOdot6\n            \"\n          />\n          <h6\n            class=\"\n              text-white\n              position-absolute\n              top-50\n              start-50\n              translate-middle\n              objectFitCover\n            \"\n          >\n            {{ item.name }}\n          </h6>\n        </div>\n      </div>\n    </div>\n  </section>\n\n  <!-- categoriesList - end -->\n  <!-- searching result - start -->\n\n  <section class=\"container\" v-if=\"showResults\">\n    <h2 class=\"fs-4 fsLg1dot5 fw-normal m-0 mb-0dot5 mb-lg-0dot75\">搜尋結果</h2>\n    <div class=\"row\">\n      <div\n        class=\"col-12 col-lg-3 mb-1dot25 mb-2dot25\"\n        v-for=\"(item, i) in resultSpotsList\"\n        :key=\"i\"\n      >\n        <CardWithImgAndName\n          class=\"w-100\"\n          :apiType=\"$route.query.theme\"\n          :spotId=\"item.ID\"\n          :imgUrl=\"item.Picture.PictureUrl1 || backupImg\"\n          :backupImg=\"backupImg\"\n          :spotName=\"item.Name\"\n          :spotCity=\"item.City\"\n        />\n      </div>\n    </div>\n  </section>\n\n  <!-- searching result - end -->\n</template>\n<script>\n// 自製的 mixin\nimport getTdxDataMixin from \"@/mixins/getTdxDataMixin\";\nimport CardWithImgAndName from \"@/components/cardWithImgAndName.vue\";\nimport Breadcrumb from \"@/components/breadcrumb.vue\";\nimport mainImg from \"@/mixins/directives/mainImg.js\";\n\nexport default {\n  name: \"first\",\n  components: { CardWithImgAndName, Breadcrumb },\n  mixins: [getTdxDataMixin, mainImg],\n  props: [\"apiType\", \"searchingParameters\"],\n  directives: {},\n  data() {\n    return {\n      keywords: \"\",\n      srcUrl: \"666\",\n      spotList: [],\n      categoriesList: [],\n      cityList: [],\n      resultSpotsList: [],\n      showResults: false,\n      apiParameters: {},\n      city: \"\",\n      category: \"\",\n      backupImg: require(\"../assets/imgForNoImgurl.png\"),\n\n      moreActivitiesList: [],\n      moreScenicSpotsList: [],\n      moreRestaurantsList: [],\n    };\n  },\n  methods: {\n    // 簡單粗暴取得所有景點\n    getAllSpotsOfTheTheme() {\n      var _this = this;\n      return (async function () {\n        return await _this.getTdxData({\n          apiType: _this.$route.query.theme,\n          orderby: [\"SrcUpdateTime desc\"],\n        });\n      })();\n    },\n    getClassListOfTheTheme() {\n      var _this = this;\n      // 整理類別\n      (async function () {\n        // 先粗暴取得所有景點（或餐廳 或活動）\n        _this.spotList = await _this.getAllSpotsOfTheTheme();\n        console.log(_this.spotList);\n        // 再將取得的景點依照其類別做分類\n        await _this.spotList.forEach(function (spot) {\n          // 整理該景點的類別\n          let classList = [spot.Class, spot.Class1, spot.Class2, spot.Class3];\n          // 從該景點的所有類別中\n          classList.forEach(function (item) {\n            if (item) {\n              // 檢查此類別是不是已經存在我們的 categoriesList 裡\n              let doesCatExist = _this.categoriesList.find((cat) => {\n                return cat.name === item;\n              });\n              // 如果類別已經存在\n              if (doesCatExist) {\n                // 檢查景點是不是已經存在該類別裡\n                let doesSpotExistInTheCat = doesCatExist.spots.find(\n                  (spotOfCat) => {\n                    return spotOfCat.ID === spot.ID;\n                  }\n                );\n                // 如果景點尚未存在該類別裡\n                if (!doesSpotExistInTheCat) {\n                  // 利用 object by reference 的特性，將該景點加入既有類別\n                  doesCatExist.spots.push(spot);\n                }\n              }\n              // 如果類別為全新，從未收錄在 我們的 categoriesList 裡\n              else {\n                // 則新增此類別，並將景點加入此新增類別之中\n                _this.categoriesList.push({ name: item, spots: [spot] });\n              }\n            }\n          });\n        });\n        console.log(await _this.categoriesList);\n\n        _this.categoriesList.forEach(function (cat) {\n          // 利用 find 找到有圖片的 spot 物件\n          let spotWithImg = cat.spots.find((spot) => {\n            return spot.Picture.PictureUrl1;\n          });\n          cat.imgUrl = spotWithImg\n            ? spotWithImg.Picture.PictureUrl1\n            : _this.backupImg;\n        });\n      })();\n    },\n    setCityParameterForGetTdxData(item) {\n      this.city = item.CityName;\n      this.apiParameters.city = item.City;\n    },\n    setClassParameterForGetTdxData(category) {\n      this.category = category;\n      switch (this.$route.query.theme) {\n        case \"ScenicSpot\":\n          this.apiParameters[\"filter\"] = [\n            `Class1%20eq%20'${category}'%20or%20Class2%20eq%20'${category}'%20or%20Class3%20eq%20'${category}'`,\n          ];\n          break;\n        case \"Restaurant\":\n          this.apiParameters[\"filter\"] = [`Class%20eq%20'${category}'`];\n          break;\n        case \"Activity\":\n          this.apiParameters[\"filter\"] = [\n            `Class1%20eq%20'${category}'%20or%20Class2%20eq%20'${category}'`,\n          ];\n          break;\n      }\n    },\n    async getSearchingResults() {\n      var _this = this;\n      console.log(\"searching\", _this.keywords);\n      _this.apiParameters[\"apiType\"] = _this.$route.query.theme;\n      _this.resultSpotsList = await _this.getTdxData(_this.apiParameters);\n      if (_this.keywords) {\n        _this.resultSpotsList = _this.resultSpotsList.filter(function (spot) {\n          var reg = new RegExp(_this.keywords.split().join(\"*\"));\n          return reg.test(spot.Name);\n        });\n      }\n      console.log(_this.resultSpotsList);\n      this.showResults = true;\n      console.log(this.apiParameters);\n      // 清空 apiParameters\n      // this.apiParameters = {};\n    },\n    getResultsOfSpecificCategory(item) {\n      this.resultSpotsList = item.spots;\n      this.showResults = true;\n      console.log(this.resultSpotsList);\n    },\n  },\n  created() {\n    var _this = this;\n    (async function () {\n      _this.cityList = await _this.getCityList();\n    })();\n\n    // 如果進入搜尋頁面時，已有指定的搜尋參數\n    // 例如當使用者從 spotDetail 的 附近美食btn 點進來時\n    // 依據搜尋參數（例如美食與座標）取得搜尋結果\n    this.getClassListOfTheTheme();\n    if (this.$route.query.lat) {\n      this.apiParameters[\"spatialFilter\"] = [\n        `nearby(${this.$route.query.lat},${this.$route.query.lon},1000)`,\n      ];\n      this.getSearchingResults();\n      this.apiParameters = {};\n    } else if (this.$route.query.keywords) {\n      this.keywords = this.$route.query.keywords;\n      this.getSearchingResults();\n    }\n  },\n};\n</script>\n<style lang=\"scss\">\n@import \"@/assets/style/scss/_utilities.scss\";\n$color-primary: #7f977b;\n\n.textUnderline {\n  text-decoration: underline #e0da48;\n}\n.letterSpacingLg {\n  letter-spacing: 1rem;\n}\n.focusPrimary:focus {\n  box-shadow: 0 0 0 0.2rem rgba($color-primary, 0.25);\n}\n* {\n  // border: 1px solid #000;\n}\n</style>\n"],"sourceRoot":""}]}