ACL,WAAO;AACL,MAAA,SAAS,EAAE,KAAK,OAAL,GAAa,IADnB;AAEL,MAAA,WAAW,EAAE,KAAK,OAAL,GAAa;AAFrB,KAAP;AAID,GAZY;AAab,EAAA,OAba,qBAaH,CAAE;AAbC,CAAf","sourcesContent":["<template>\n  <swiper\n    v-if=\"carsouelList\"\n    :slidesPerView=\"1\"\n    :spaceBetween=\"30\"\n    :loop=\"true\"\n    :pagination=\"{\n      clickable: true,\n    }\"\n    :navigation=\"true\"\n    class=\"position-relative\"\n  >\n    <swiper-slide\n      v-for=\"(spot, i) in carsouelList\"\n      :key=\"i\"\n      class=\"\n        paddingTop53Percent\n        paddingTopLg36Percent\n        position-relative\n        text-white\n        radius24px\n        overflow-hidden\n      \"\n    >\n      <router-link\n        v-if=\"areWordsOnImg\"\n        :to=\"'/spot?theme=' + apiType + '&id=' + spot[spotIDKey]\"\n        target=\"_blank\"\n        class=\"\n          w-100\n          h-100\n          d-flex\n          justify-content-center\n          align-items-center\n          position-absolute\n          top-50\n          start-50\n          translate-middle\n          zIndex1\n        \"\n      >\n        <h3 class=\"text-white fs-5 fsLg3\">{{ spot.City }} | {{ spot[spotNameKey] }}</h3>\n      </router-link>\n      <img\n        :src=\"spot.Picture.PictureUrl1\"\n        :alt=\"spot.PictureDescription1 || spot[spotNameKey]\"\n        class=\"\n          h-100\n          w-100\n          position-absolute\n          top-50\n          start-0\n          translate-middle-y\n          objectFitCover\n        \"\n    /></swiper-slide>\n  </swiper>\n</template>\n\n<script>\n// Import Swiper Vue.js components\n// 雖然官方寫： import { Swiper, SwiperSlide } from 'swiper/vue';\n// 但用官網的寫法 import 失敗後，主要還是要觀察 module folder 裡的 swiper/vue 結構;\nimport { Swiper, SwiperSlide } from \"swiper/vue/swiper-vue\";\n\n// import Swiper core and required modules\nimport SwiperCore, { Pagination, Navigation } from \"swiper\";\n\n// install Swiper modules\nSwiperCore.use([Pagination, Navigation]);\n\n// Import Swiper styles\n// 雖然官方寫： import 'swiper/css';\n// 但用官網的寫法 import 失敗後，主要還是要觀察 module folder 裡的 swiper 結構\nimport \"swiper/swiper-bundle.css\";\n\nexport default {\n  name: \"carsouel\",\n  components: {\n    Swiper,\n    SwiperSlide,\n  },\n  props: [\"areWordsOnImg\", \"apiType\", \"carsouelList\"],\n  data() {\n    return {\n      spotIDKey: this.apiType+'ID',\n      spotNameKey: this.apiType+'Name',\n    };\n  },\n  mounted() {},\n};\n</script>\n"],"sourceRoot":""}]}