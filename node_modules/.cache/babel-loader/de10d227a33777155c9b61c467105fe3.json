KAAK,EAAC;;;;AAyBJ,EAAA,KAAK,EAAC;;;AAGP,EAAA,KAAK,EAAC;;;;sBACP,mBAAA,CAIC,MAJD,EAIC;AAHC,IAAA,KAAK,EAAC;AAGP,GAJD,EAEC,SAFD,EAGQ,CAAA;AAAA;AAHR,G;;;;;;;;SAlDA,KAAA,CAAA,YAAA,CAAY,CAAZ,C,kBADR,YAAA,CA2DS,iBA3DT,EA2DS;UAAA;AAzDN,IAAA,aAAa,EAAE,GAyDT;AAxDN,IAAA,YAAY,EAAE,EAwDR;AAvDN,IAAA,WAAW,EAAE;;;;OAAA;;;;;AAAA,KAuDP;AA7CP,IAAA,KAAK,EAAC;AA6CC,GA3DT,E;sBAgBgB;AAAA,aAAiC,E,kBAA/C,mBAAA,CA0Ce,SA1Cf,EA0Ce,IA1Cf,EA0Ce,WAAA,CA1CmB,KAAA,CAAA,YA0CnB,EA1C+B,UAAxB,IAAwB,EAAlB,CAAkB,EAAjB;6BAA7B,YAAA,CA0Ce,uBA1Cf,EA0Ce;AA1CkC,UAAA,GAAG,EAAE;AA0CvC,SA1Cf,EAAuD;4BACrD;AAAA,mBAwCI,CAxCJ,mBAAA,CAwCI,GAxCJ,EAAA,UAAA,EAwCI,CApCF,mBAAA,CAyBM,KAzBN,EAAA,UAAA,EAyBM,CAbJ,mBAAA,CAYE,KAZF,EAYE;AAXC,cAAA,GAAG,EAAE,IAAI,CAAC,OAAL,CAAa,WAWnB;AAVC,cAAA,GAAG,EAAE,IAAI,CAAC,IAUX;AATA,cAAA,KAAK,EAAC;AASN,aAZF,E,IAAA,E;;AAAA,c,UAAA,CAaI,CAzBN,CAoCE,EAVF,mBAAA,CAEK,IAFL,EAAA,UAAA,EAEK,gBAAA,CADA,IAAI,CAAC,IACL,CAFL,EACc;AAAA;AADd,aAUE,EAPF,mBAAA,CAMI,GANJ,EAAA,UAAA,EAMI,CALF,UAKE,E,kCADE,IAAI,CAAC,O,GAAO;AAAA;aACd,CANJ,CAOE,CAxCJ,CAwCI,CAxCJ;AAAA,W,CADqD;;;;AAAA,SAAvD,E;;AAAA,S;OA0Ce,CA1Cf,E;;AAAA,OAA+C,EAAjC;AAAA,K;;;;GAhBhB,E;;AAAA,I,gCAAA,C","sourcesContent":["<template>\n  <swiper\n    v-if=\"carsouelList[0]\"\n    :slidesPerView=\"1.5\"\n    :spaceBetween=\"16\"\n    :breakpoints=\"{\n      '768': {\n        slidesPerView: 2.5,\n        spaceBetween: 30,\n      },\n      '992': {\n        slidesPerView: 4,\n        spaceBetween: 30,\n      },\n    }\"\n    class=\"position-relative\"\n  >\n    <swiper-slide v-for=\"(spot, i) in carsouelList\" :key=\"i\">\n      <a\n        href=\"https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTD6oeupr4OOzfY53759FOeS6fHA0p9O_SAXA&usqp=CAU\"\n        target=\"_blank\"\n      >\n        <div\n          class=\"\n            w-100\n            h-100\n            paddingTop75Percent\n            position-relative\n            radius20px\n            overflow-hidden\n            hoverImgBigger\n            mb-0dot375 mb-lg-0dot625\n          \"\n        >\n          <img\n            :src=\"spot.Picture.PictureUrl1\"\n            :alt=\"spot.Name\"\n            class=\"\n              position-absolute\n              w-100\n              h-100\n              top-50\n              start-0\n              translate-middle-y\n              objectFitCover\n            \"\n          />\n        </div>\n        <h5 class=\"fs-5dot125 fsLg4dot375 text-black\">\n          {{ spot.Name }}\n        </h5>\n        <p class=\"m-0 text-black-light fs-6dot75 fsLg6\">\n          <span\n            class=\"align-text-bottom material-icons-outlined d-inline-block\"\n          >\n            place </span\n          >{{ spot.ZipCode }}\n        </p>\n      </a>\n    </swiper-slide>\n  </swiper>\n</template>\n\n<script>\nimport jsSHA from \"jssha\";\nimport axios from \"axios\";\n\n// Import Swiper Vue.js components\n// 雖然官方寫： import { Swiper, SwiperSlide } from 'swiper/vue';\n// 但用官網的寫法 import 失敗後，主要還是要觀察 module folder 裡的 swiper/vue 結構;\nimport { Swiper, SwiperSlide } from \"swiper/vue/swiper-vue\";\n\n// import Swiper core and required modules\nimport SwiperCore, { Pagination } from \"swiper\";\n\n// install Swiper modules\nSwiperCore.use([Pagination]);\n\n// Import Swiper styles\n// 雖然官方寫： import 'swiper/css';\n// 但用官網的寫法 import 失敗後，主要還是要觀察 module folder 裡的 swiper 結構\nimport \"swiper/swiper-bundle.css\";\n\nexport default {\n  name: \"CarouselOnMobile\",\n  components: {\n    Swiper,\n    SwiperSlide,\n  },\n  props: {},\n  data() {\n    return {\n      carsouelList: [],\n    };\n  },\n  mounted() {\n    function getAuthorizationHeader() {\n      //  填入自己 ID、KEY 開始\n      let AppID = \"6881a6e19c3240089c9d8cc87f52f52e\";\n      let AppKey = \"z274IyT03M2HixJY5cAbZn-8ccs\";\n      //  填入自己 ID、KEY 結束\n\n      let GMTString = new Date().toGMTString();\n      let ShaObj = new jsSHA(\"SHA-1\", \"TEXT\");\n      ShaObj.setHMACKey(AppKey, \"TEXT\");\n      ShaObj.update(\"x-date: \" + GMTString);\n      let HMAC = ShaObj.getHMAC(\"B64\");\n      let Authorization = `hmac username=\"${AppID}\",algorithm=\"hmac-sha1\",headers=\"x-date\",signature=\"${HMAC}\"`;\n      return { Authorization: Authorization, \"X-Date\": GMTString };\n    }\n    var _this = this;\n\n    axios\n      .get(\n        // $select     Picture, Name, Address, ZipCode\n        // $orderby    SrcUpdateTime desc; 排序按照資料更新日期最新的開始\n        // $top        12; 取 12 筆資料\n        // $skip\t     10; 跳過前 10 筆資料\n        // 取得觀光景點資料\n        \"https://ptx.transportdata.tw/MOTC/v2/Tourism/ScenicSpot?$select=Picture%2C%20Name%2C%20Address%2C%20ZipCode&$orderby=SrcUpdateTime%20desc&$top=12&$skip=10&$format=JSON\",\n        {\n          headers: getAuthorizationHeader(),\n        }\n      )\n      .then(function (res) {\n        // 只將有照片的景點存到景點清單\n        res.data.forEach(function (spot) {\n          if (spot.Picture.PictureUrl1) {\n            _this.carsouelList.push(spot);\n          }\n        });\n      })\n      .catch(function (error) {\n        console.log(error);\n      });\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\n@import \"@/assets/style/scss/_utilities.scss\";\n\n.height160px {\n  height: 160px;\n}\n@media (min-width: 992px) {\n  .heightLg200px {\n    height: 200px;\n  }\n}\n.minWidth100 {\n  min-width: 100%;\n}\n.minHeight100 {\n  min-height: 100%;\n}\n.zIndex1 {\n  z-index: 1;\n}\n</style>\n"],"sourceRoot":""}]}