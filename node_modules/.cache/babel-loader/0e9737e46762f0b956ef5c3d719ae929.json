CAAX;AAIA,MAAA,CAAC,CAAC,SAAF,CACE,oFADF,EAEE;AACE,QAAA,WAAW,EACT,0JAFJ;AAGE,QAAA,OAAO,EAAE,EAHX;AAIE;AACA,QAAA,EAAE,EAAE,oBALN;AAME,QAAA,QAAQ,EAAE,GANZ;AAOE,QAAA,UAAU,EAAE,CAAC,CAPf;AAQE,QAAA,WAAW,EACT;AATJ,OAFF,EAaE,KAbF,CAaQ,KAAK,GAbb;AAcD,KArBM;AAsBP,IAAA,aAtBO,yBAsBO,KAtBP,EAsBc,GAtBd,EAsBmB,GAtBnB,EAsBwB;AAC7B;AACA,WAAK,gBAAL,CAAsB,KAAtB,EAA6B,GAA7B,EAAkC,GAAlC,EAF6B,CAI7B;;AACA,UAAI,MAAK,GAAI,CAAC,CAAC,OAAF,CAAU;AACrB;AACA,QAAA,IAAI,sEAFiB;AAKrB;AACA,QAAA,QAAQ,EAAE,CAAC,EAAD,EAAK,EAAL,CANW;AAMD;AACpB,QAAA,UAAU,EAAE,CAAC,EAAD,EAAK,EAAL,CAPS;AAOC;AACtB,QAAA,WAAW,EAAE,CAAC,CAAD,EAAI,CAAC,EAAL,CARQ,CAQE;;AARF,OAAV,CAAb,CAL6B,CAgB7B;AACA;;AACA,MAAA,CAAC,CAAC,MAAF,CAAS,CAAC,GAAD,EAAM,GAAN,CAAT,EAAqB;AAAE,QAAA,IAAI,EAAE;AAAR,OAArB,EAAuC,KAAvC,CAA6C,KAAK,GAAlD;AACD;AAzCM,GARI;AAmDb,EAAA,OAnDa,qBAmDH;AACR;AACA,SAAK,aAAL,CAAmB,KAAK,KAAxB,EAA+B,KAAK,GAApC,EAAyC,KAAK,GAA9C;AACD;AAtDY,CAAf","sourcesContent":["<template>\n  <div :id=\"mapId\"></div>\n</template>\n<script>\n// 在地圖上顯示 TDX 資料\nimport L from \"leaflet\";\n\n// 自製的 mixin\n\nexport default {\n  name: \"map\",\n  props: [\"mapId\", \"lat\", \"lon\"],\n  data() {\n    return {\n      map: {},\n    };\n  },\n  methods: {\n    initalLeafletMap(mapId, posLat, posLon) {\n      // inital map\n      this.map = L.map(mapId, {\n        center: [posLat, posLon],\n        zoom: 18,\n      });\n      L.tileLayer(\n        \"https://api.mapbox.com/styles/v1/{id}/tiles/{z}/{x}/{y}?access_token={accessToken}\",\n        {\n          attribution:\n            'Map data &copy; <a href=\"https://www.openstreetmap.org/copyright\">OpenStreetMap</a> contributors, Imagery © <a href=\"https://www.mapbox.com/\">Mapbox</a>',\n          maxZoom: 25,\n          // id:\"mapbox/satellite-v9\" 為實景空拍模式\n          id: \"mapbox/streets-v11\",\n          tileSize: 512,\n          zoomOffset: -1,\n          accessToken:\n            \"pk.eyJ1IjoibG9ic3RlcjAxIiwiYSI6ImNrdXdvN3prYTUyYXgyb3BoaThwcmcxN2cifQ.CsAL1uSEtZjpGwuEpYA-6A\",\n        }\n      ).addTo(this.map);\n    },\n    showSpotOnMap(mapId, lat, lon) {\n      // 先初始化將 leaflet 撲到畫面上\n      this.initalLeafletMap(mapId, lat, lon);\n\n      // 客製化 icon，可以自己設計 html\n      var myIcon = L.divIcon({\n        // 客製化 icon\n        html: `<span class=\"material-icons locationIcon\">\nlocation_on\n</span>`,\n        // 給定 icon 大小\n        iconSize: [41, 51], // size of the icon\n        iconAnchor: [20, 51], // point of the icon which will correspond to marker's location\n        popupAnchor: [0, -51], // point from which the popup should open relative to the iconAnchor\n      });\n\n      // 讓 marker 使用我們設計的 客製化 icon\n      // 並在指定經緯度放上客製化的 marker\n      L.marker([lat, lon], { icon: myIcon }).addTo(this.map);\n    },\n  },\n  mounted() {\n    // console.log(this.mapId, this.lat, this.lon);\n    this.showSpotOnMap(this.mapId, this.lat, this.lon);\n  },\n};\n</script>\n<style lang=\"scss\">\n@import \"@/assets/style/scss/_color.scss\";\n\n.locationIcon {\n  color: $color-secondary;\n  font-size: 48px;\n}\n</style>\n"],"sourceRoot":""}]}