t>\n// 自製的 mixin\nimport getTdxDataMixin from \"@/mixins/getTdxDataMixin\";\nimport CardWithImgAndName from \"@/components/cardWithImgAndName.vue\";\nimport Breadcrumb from \"@/components/breadcrumb.vue\";\n\nexport default {\n  name: \"first\",\n  components: { CardWithImgAndName, Breadcrumb },\n  mixins: [getTdxDataMixin],\n  props: [\"apiType\", \"searchingParameters\"],\n  data() {\n    return {\n      spotList: [],\n      categoriesList: [],\n      cityList: [],\n      resultSpotsList: [],\n      showResults: false,\n      apiParameters: {},\n      city: \"\",\n      category: \"\",\n\n      moreActivitiesList: [],\n      moreScenicSpotsList: [],\n      moreRestaurantsList: [],\n    };\n  },\n  methods: {\n    getClassListOfTheTheme() {\n      var _this = this;\n      // 整理類別\n      (async function () {\n        // 先粗暴取得所有景點（或餐廳 或活動）\n        _this.spotList = await _this.getTdxData({\n          apiType: _this.$route.query.theme,\n          orderby: [\"SrcUpdateTime desc\"],\n        });\n        // 再將取得的景點依照其類別做分類\n        await _this.spotList.forEach(function (spot) {\n          // 整理該景點的類別\n          let classList = [spot.Class, spot.Class1, spot.Class2, spot.Class3];\n          // 從該景點的所有類別中\n          classList.forEach(function (item) {\n            if (item) {\n              // 檢查此類別是不是已經存在我們的 categoriesList 裡\n              let doesCatExist = _this.categoriesList.find((cat) => {\n                return cat.name === item;\n              });\n              // 如果類別已經存在\n              if (doesCatExist) {\n                // 利用 object by reference 的特性，將該景點加入既有類別\n                doesCatExist.spots.push(spot);\n              }\n              // 如果類別為全新，從未收錄在 我們的 categoriesList 裡\n              else {\n                // 則新增此類別，並將景點加入此新增類別之中\n                _this.categoriesList.push({ name: item, spots: [spot] });\n              }\n            }\n          });\n        });\n        console.log(await _this.categoriesList);\n\n        _this.categoriesList.forEach(function (cat) {\n          // 利用 find 找到有圖片的 spot 物件\n          let spotWithImg = cat.spots.find((spot) => {\n            return spot.Picture.PictureUrl1;\n          });\n          cat.imgUrl = spotWithImg ? spotWithImg.Picture.PictureUrl1 : \"../src/assets/imgForNoImgurl.png\";\n        });\n      })();\n    },\n    setCityForSearching(item) {\n      this.city = item.CityName;\n      this.apiParameters.city = item.City;\n    },\n    setCategoryForSearching(category) {\n      this.category = category;\n      switch (this.$route.query.theme) {\n        case \"ScenicSpot\":\n          this.apiParameters[\"filter\"] = [\n            `Class1%20eq%20'${category}'%20or%20Class2%20eq%20'${category}'%20or%20Class3%20eq%20'${category}'`,\n          ];\n          break;\n        case \"Restaurant\":\n          this.apiParameters[\"filter\"] = [`Class%20eq%20'${category}'`];\n          break;\n        case \"Activity\":\n          this.apiParameters[\"filter\"] = [\n            `Class1%20eq%20'${category}'%20or%20Class2%20eq%20'${category}'`,\n          ];\n          break;\n      }\n    },\n    async getSearchingResults() {\n      console.log(\"searching\");\n      var _this = this;\n      _this.apiParameters[\"apiType\"] = _this.$route.query.theme;\n      _this.resultSpotsList = await _this.getTdxData(_this.apiParameters);\n      this.showResults = true;\n\n      // 清空 apiParameters\n      this.apiParameters = {};\n    },\n    getResultsOfSpecificCategory(item) {\n      this.resultSpotsList = item.spots;\n      this.showResults = true;\n      console.log(this.resultSpotsList);\n    },\n  },\n  mounted() {\n    var _this = this;\n    (async function () {\n      _this.cityList = await _this.getCityList();\n      console.log(_this.cityList[9]);\n    })();\n\n    this.getClassListOfTheTheme();\n\n    // 如果進入搜尋頁面時，已有指定的搜尋參數\n    // 例如當使用者從 spotDetail 的 附近美食btn 點進來時\n    // 依據搜尋參數（例如美食與座標）取得搜尋結果\n    if (this.searchingParameters) {\n      this.apiParameters = this.searchingParameters;\n      this.getSearchingResults();\n    }\n    /*\n        var _this = this;\n    (async function () {\n      let scenicSpotsList = await _this.getTdxData({\n        apiType: \"ScenicSpot\",\n        orderby: [\"SrcUpdateTime desc\"],\n        top: [\"20\"],\n        skip: [\"50\"],\n      });\n      await scenicSpotsList.forEach(function (item) {\n        if (item.Picture.PictureUrl1) {\n          if (_this.carsouelList.length < 6) {\n            _this.carsouelList.push(item);\n          } else {\n            _this.moreScenicSpotsList.push(item);\n          }\n        }\n      });\n    })();\n    (async function () {\n      let activitiesList = await _this.getTdxData({\n        apiType: \"Activity\",\n        orderby: [\"SrcUpdateTime desc\"],\n        top: [\"30\"],\n      });\n      await activitiesList.forEach(function (item) {\n        if (item.Picture.PictureUrl1 && _this.moreActivitiesList.length < 4) {\n          item.EndTime = item.EndTime.slice(0, 10);\n          item.StartTime = item.StartTime.slice(0, 10);\n          _this.moreActivitiesList.push(item);\n        }\n      });\n      console.log(_this.moreActivitiesList);\n    })();\n    */\n  },\n};\n</script>\n<style lang=\"scss\">\n@import \"@/assets/style/scss/_utilities.scss\";\n$color-primary: #7f977b;\n\n.textUnderline {\n  text-decoration: underline #e0da48;\n}\n.letterSpacingLg {\n  letter-spacing: 1rem;\n}\n.focusPrimary:focus {\n  box-shadow: 0 0 0 0.2rem rgba($color-primary, 0.25);\n}\n* {\n  // border: 1px solid #000;\n}\n</style>\n"]}]}