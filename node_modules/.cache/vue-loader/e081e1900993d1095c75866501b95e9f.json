div class=\"row\" v-if=\"spotsList[0]\">\n    <a\n      class=\"col col-12 col-lg-6 d-flex mb-0dot75 hoverImgBigger\"\n      v-for=\"(spot, i) in spotsList\"\n      :key=\"i\"\n      href=\"https://google.com\"\n      target=\"_blank\"\n    >\n      <div\n        class=\"\n          col-4\n          position-relative\n          overflow-hidden\n          paddingTopLg30Percent\n          paddingTop20Percent\n          radiusLg12px\n          radius8px\n          radiusLgEndNone\n          border border-gray\n        \"\n      >\n        <!-- :src=\"spot.Picture.PictureUrl1\" -->\n        <img\n          class=\"\n            h-100\n            w-100\n            objectFitCover\n            position-absolute\n            top-50\n            start-50\n            translate-middle\n          \"\n          :src=\"spot.Picture.PictureUrl1\"\n          :alt=\"spot.Name\"\n        />\n      </div>\n      <div\n        class=\"\n          col-8\n          radiusLg12px\n          radiusLgStartNone\n          p-0\n          ps-1\n          py-lg-1\n          ps-lg-2\n          pe-lg-1dot5\n          d-flex\n          flex-column\n          justify-content-between\n          bgColorLgGrayLight\n          borderLgGray\n        \"\n      >\n        <div>\n          <p class=\"mb-0dot25 text-black-light fs-6dot75 fsLg6\">\n            {{ spot.StartTime }} - {{ spot.EndTime }}\n          </p>\n          <h5 class=\"textOverFlowHidden fs-6 fsLg4dot375\">{{ spot.Name }}</h5>\n        </div>\n        <div\n          class=\"\n            d-flex\n            justify-content-between\n            align-items-end\n            position-relative\n          \"\n        >\n          <p class=\"m-0 text-black-light fs-6dot75 fsLg6\">\n            <span\n              class=\"\n                align-text-bottom\n                material-icons-outlined\n                d-none d-lg-inline-block\n              \"\n            >\n              place </span\n            >{{ spot.City }}\n          </p>\n          <a\n            class=\"link-primary stretched-link d-none d-lg-inline\"\n            href=\"https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSwTibxpHQtnHzTgHzkyi_JZZgUfufJ9r6g9w&usqp=CAU\"\n            target=\"_blank\"\n            >詳細介紹 ❯</a\n          >\n        </div>\n      </div>\n    </a>\n  </div>\n</template>\n\n<script>\nimport jsSHA from \"jssha\";\nimport axios from \"axios\";\n\nexport default {\n  name: \"carsouel\",\n  components: {},\n  props: {},\n  data() {\n    return {\n      spotsList: [],\n    };\n  },\n  mounted() {\n    function getAuthorizationHeader() {\n      //  填入自己 ID、KEY 開始\n      let AppID = \"6881a6e19c3240089c9d8cc87f52f52e\";\n      let AppKey = \"z274IyT03M2HixJY5cAbZn-8ccs\";\n      //  填入自己 ID、KEY 結束\n\n      let GMTString = new Date().toGMTString();\n      let ShaObj = new jsSHA(\"SHA-1\", \"TEXT\");\n      ShaObj.setHMACKey(AppKey, \"TEXT\");\n      ShaObj.update(\"x-date: \" + GMTString);\n      let HMAC = ShaObj.getHMAC(\"B64\");\n      let Authorization = `hmac username=\"${AppID}\",algorithm=\"hmac-sha1\",headers=\"x-date\",signature=\"${HMAC}\"`;\n      return { Authorization: Authorization, \"X-Date\": GMTString };\n    }\n    var _this = this;\n\n    axios\n      .get(\n        // $select     Picture, Name, Address, ZipCode\n        // $orderby    SrcUpdateTime desc; 排序按照資料更新日期最新的開始\n        // $top        12; 取 12 筆資料\n        // $skip\t     10; 跳過前 10 筆資料\n        // 取得觀光景點資料\n        \"https://ptx.transportdata.tw/MOTC/v2/Tourism/Activity?$select=ID%2C%20Name%2C%20Description%2C%20Address%2C%20City%2C%20StartTime%2C%20EndTime%2C%20Picture&$orderby=StartTime%20desc&$top=30&$format=JSON\",\n        {\n          headers: getAuthorizationHeader(),\n        }\n      )\n      .then(function (res) {\n        // 只將有照片的景點存到景點清單\n        res.data.forEach(function (item) {\n          if (_this.spotsList.length < 4 && item.Picture.PictureUrl1) {\n            item.EndTime = item.EndTime.slice(0, 10);\n            item.StartTime = item.StartTime.slice(0, 10);\n            _this.spotsList.push(item);\n          }\n        });\n        console.log(_this.spotsList);\n      })\n      .catch(function (error) {\n        console.log(error);\n      });\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\n@import \"@/assets/style/scss/_utilities.scss\";\n\n\n* {\n  // border: 1px solid black;\n}\n\n\n</style>\n"]}]}